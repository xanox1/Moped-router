{
  "name": "moped-router",
  "version": "1.0.0",
  "description": "GraphHopper Moped Routing Server with Web and Android clients",
  "main": "web/script.js",
  "scripts": {
    "dev": "cd web && python3 -m http.server 3000",
    "test": "jest",
    "lint": "eslint web/script.js",
    "build": "npm run build:web",
    "build:web": "cp -r web/ dist/",
    "serve": "cd web && python3 -m http.server 3000"
  },
  "keywords": [
    "routing",
    "moped",
    "graphhopper",
    "maps",
    "navigation",
    "android"
  ],
  "author": "xanox1",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^23.0.1"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
