{
  "speed": [
    {
      "if": "true",
      "limit_to": "45"
    },
    {
      "if": "zone_maxspeed > 0 && zone_maxspeed < 45",
      "limit_to": "zone_maxspeed"
    }
  ],
  "priority": [
    {
      "if": "road_class == MOTORWAY || road_class == TRUNK",
      "multiply_by": "0.1"
    },
    {
      "if": "road_class == PRIMARY",
      "multiply_by": 0
    },
    {
      "if": "moped == no",
      "multiply_by": 0
    },
    {
      "if": "motor_vehicle == no",
      "multiply_by": 0
    },
    {
      "if": "vehicle == no",
      "multiply_by": 0
    },
    {
      "if": "max_speed > 45",
      "multiply_by": 0
    },
    {
      "if": "zone_maxspeed > 45",
      "multiply_by": 0
    },
    {
      "if": "traffic_sign == 'NL:C5'",
      "multiply_by": 0
    },
    {
      "if": "traffic_sign == 'NL:C2'",
      "multiply_by": 0
    },
    {
      "if": "traffic_sign == 'NL:C7'",
      "multiply_by": 0
    },
    {
      "if": "traffic_sign == 'NL:C1'",
      "multiply_by": 0
    },
    {
      "if": "traffic_sign == 'NL:C12'",
      "multiply_by": 0
    },
    {
      "if": "cycleway_moped == designated || traffic_sign == 'NL:G12a'",
      "multiply_by": "1.5"
    },
    {
      "if": "cycleway_moped == yes",
      "multiply_by": "1.2"
    }
  ],
  "distance_influence": [
    {
      "if": "road_class == PRIMARY",
      "multiply_by": 1000
    },
    {
      "if": "moped == no",
      "multiply_by": 1000
    },
    {
      "if": "motor_vehicle == no",
      "multiply_by": 1000
    },
    {
      "if": "vehicle == no",
      "multiply_by": 1000
    },
    {
      "if": "max_speed > 45",
      "multiply_by": 1000
    },
    {
      "if": "zone_maxspeed > 45",
      "multiply_by": 1000
    },
    {
      "if": "traffic_sign == 'NL:C5'",
      "multiply_by": 1000
    },
    {
      "if": "traffic_sign == 'NL:C2'",
      "multiply_by": 1000
    },
    {
      "if": "traffic_sign == 'NL:C7'",
      "multiply_by": 1000
    },
    {
      "if": "traffic_sign == 'NL:C1'",
      "multiply_by": 1000
    },
    {
      "if": "traffic_sign == 'NL:C12'",
      "multiply_by": 1000
    },
    {
      "if": "cycleway_moped == designated || traffic_sign == 'NL:G12a'",
      "multiply_by": "0.8"
    },
    {
      "if": "cycleway_moped == yes",
      "multiply_by": "0.9"
    }
  ]
}
